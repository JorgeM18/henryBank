{
  "name": "api",
  "version": "1.0.0",
  "description": "My Moleculer-based microservices project",
  "scripts": {
    "dev": "moleculer-runner --repl --hot services/**/*.service.js",
    "start": "moleculer-runner",
    "cli": "moleculer connect NATS",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint services",
    "dc:up": "docker-compose up --build -d",
    "dc:logs": "docker-compose logs -f",
    "dc:down": "docker-compose down"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "eslint": "^7.7.0",
    "jest": "^26.4.2",
    "jest-cli": "^26.4.2",
    "moleculer-repl": "^0.6.4"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "firebase": "^8.1.1",
    "handlebars": "^4.7.6",
    "jsonwebtoken": "^8.5.1",
    "juice": "^7.0.0",
    "mercadopago": "^1.5.0",
    "moleculer": "^0.14.10",
    "moleculer-db": "^0.8.4",
    "moleculer-db-adapter-mongo": "^0.4.7",
    "moleculer-web": "^0.9.0",
    "moment": "^2.29.1",
    "nats": "^1.4.12",
    "nodemailer": "^6.4.15",
    "nodemailer-express-handlebars": "^4.0.0",
    "open": "^7.3.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "paypal-rest-sdk": "^1.8.1",
    "pg": "^8.4.2",
    "sequelize": "^6.3.5",
    "stripe": "^8.126.0",
    "twilio": "^3.30.0"
  },
  "engines": {
    "node": ">= 10.x.x"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
}
